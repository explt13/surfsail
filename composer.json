{
    "name": "explt13/surfsail",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/nosmi/",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Surfsail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require": {
        "phpmailer/phpmailer": "6.9.3",
        "vlucas/phpdotenv": "5.6.1",
        "respect/validation": "2.3.11",
        "explt13/nosmi": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    }
}
